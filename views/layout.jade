doctype html
html(ng-app='getBookin')
  head
    title= title

    // ---- Bootstrap & Plugins ----
    link(rel='stylesheet', href='/libraries/bootstrap/css/bootstrap.min.css', type='text/css')
    link(rel='stylesheet', href='//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css', type='text/css')

    //link(rel='stylesheet', href='/stylesheets/global.css', type='text/css')

    // ---- AngularJS & Plugins ----
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.2.4/angular.min.js')
    //script(src='http://code.angularjs.org/1.2.4/angular-route.min.js')

    block additionalCss

  body
    div(id='fb-root')

    // -------- [START] Navbar --------
    nav(class='navbar navbar-default navbar-fixed-top', role='navigation')
      .navbar-header
      a.navbar-brand(href='/')
        img(src='images/logo.png', alt='LOGO')
      #main-navbar(class='collapse navbar-collapse')
        ul(class='nav navbar-nav')
          li
            a(href='/') NEW BOOKING
          li
            a(href='/') MAKE CHANGE
          li
            a(href='/') CANCEL
        ul(class='nav navbar-nav navbar-right')
          #main-navbar-signinwith SIGN IN WITH&nbsp;
            button(class='btn btn-default navbar-btn' onclick='window.location="/pages/adminSchedule";')
              i(class='fa fa-google-plus')
            fblogin
    // -------- [END] Navbar --------

    block mainContainer

    // ---- jQuery & Plugins ----
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js')


    // ---- Bootstrap & Plugins ----
    script(src='http://netdna.bootstrapcdn.com/bootstrap/3.0.2/js/bootstrap.min.js')

    // ---- Google Map ----
    script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAo0HBa1d_xNIIwx1UksjPg50MQQbjLCjQ&sensor=true')

    script(src='javascripts/global.js')
    script(src='config.js')

    // ---- Classes ----
    script(src='javascripts/class/map.js')

    // ---- Additional Libraries ----
    script(src='libraries/moment/moment.min.js')

    // ---- App JS ----
    script(src='javascripts/directives/fbLogin.js')

    block additionalJs

